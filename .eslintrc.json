{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"prettier/prettier": [
            "error",
            {
                "indent": ["error", "tab"],
                "linebreak-style": ["error", "unix"],
                "quotes": ["error", "single"],
                "semi": ["error", "always"],
                "camelcase":"error",
              "singleQuote": true,
              "parser": "flow"
            }
          ]
	}
}
